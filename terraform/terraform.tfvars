# AWS Region
aws_region = "us-east-1"

# VPC CIDR
vpc_cidr = "10.0.0.0/16"

# Public Subnet CIDRs
public_subnet_cidrs = [
  "10.0.1.0/24",
  "10.0.2.0/24"
]

# Availability Zones
availability_zones = [
  "us-east-1a",
  "us-east-1b"
]

# Environment
environment = "dev"

# Ubuntu AMI
ubuntu_ami = "ami-04552bb4f4dd38925"

# EC2 Instance Type
instance_type = "t2.small"

# EC2 Key Pair Name
key_name = "ec2-key"

# Domain Name
domain_name = "mytoolings.xyz"
